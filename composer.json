{
    "name": "sunel/eav-dashboard",
    "description": "Eav Dashboard.",
    "keywords": ["eav", "laravel"],
    "license": "MIT",
    "require": {
        "php": ">=7.0.0",
        "sunel/eav":"^0.1.0"
    },
    "authors": [
        {
            "name": "Sunel TR",
            "email": "sunelbe@gmail.com"
        }
    ],
    "autoload": {
